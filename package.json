{
  "name": "project",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "private": "true",
  "scripts": {
    "build:prod": "webpack --env mode='production'",
    "build:dev": "webpack --env mode='development'",
    "start": "webpack serve --env port=4200",
    "start:backend": "node ./json_server/index.js",
    "start:vite": "vite",
    "start:dev": "concurrently \"npm run start\" \"npm run start:backend\"",
    "start:dev:vite": "concurrently \"npm run start:vite\" \"npm run start:backend\"",
    "prettier": "npx prettier --write **/*.{ts|tsx}",
    "lint:ts": "eslint \"**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
    "test:ui:report:json": "node scripts/htmlReport.js",
    "test:ui:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "storybook:build": "build-storybook -c ./config/storybook",
    "generate:slice": "node ./scripts/createSlice/index.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier",
      "eslint"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
